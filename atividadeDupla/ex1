#include<stdio.h>
int main(){
    int n,i,contfor=0,cont1=0,divisoranterior=-5;
    printf("insira o n: ");
    scanf("%d", &n);
    for (i=2;n>i;i++){
        if (n%i==0){
            if (divisoranterior==i){
                cont1=cont1+1;}}
        if (n%i==0){
            divisoranterior=i;
            n=n/i;
            i=2;}}
    if (cont1%2==1 && cont1>0)
        printf("e quadrado perfeito");
}
